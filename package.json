{
  "name": "camv",
  "version": "0.1.1",
  "main": "src/main.js",
  "productName": "CAMV",
  "dependencies": {
    "JSONStream": "^1.3.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2017-node7": "^0.5.1",
    "babel-preset-react": "^6.23.0",
    "dom-to-image": "github:naderm/dom-to-image",
    "react": "^15.4.2",
    "react-addons-update": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-hotkey": "^0.7.0",
    "react-native-fs": "^2.1.0-rc.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "electron-forge": "^2.8.2",
    "electron-prebuilt-compile": "1.6.1",
    "eslint": "^3.17.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty src/ui/*.jsx",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "package-all": "electron-forge package --platform=all --arch=all",
    "package-win": "electron-forge package --platform=win32 --arch=all",
    "package-linux": "electron-forge package --platform=linux --arch=all",
    "package-osx": "electron-forge package --platform=darwin --arch=all",
    "make": "electron-forge make"
  },
  "files": [
    "package.json",
    "bower.json",
    "node_modules/",
    ".babelrc",
    ".bowerrc",
    "src/bower_components/",
    "src/*.css",
    "src/*.html",
    "src/*.js",
    "src/ui/*.jsx"
  ],
  "devEngines": {
    "node": ">=7.x",
    "npm": ">=4.x"
  },
  "author": {
    "name": "Nader Morshed",
    "email": "morshed@mit.edu",
    "url": "https://github.com/naderm"
  },
  "license": "MIT",
  "repository": "https://github.com/white-lab/CAMV",
  "description": "Tool for validation proteomic mass spectrometry peptide assignments.",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {},
      "electronWinstallerConfig": {
        "name": "CAMV"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "white-lab",
        "name": "CAMV"
      },
      "windowsStoreConfig": {
        "packageName": ""
      }
    }
  }
}
